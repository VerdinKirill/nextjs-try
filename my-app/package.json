{
  "type": "module",
  "name": "gravity-ui-example-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "bun run husky install",
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint:js": "bunx eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "bunx stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "bunx prettier --check '**/*.md'",
    "lint": "bun run lint:js && bun run lint:styles && bun run lint:prettier",
    "postinstall": "bun run prepare"
  },
  "dependencies": {
    "@gravity-ui/icons": "^2.12.0",
    "@gravity-ui/navigation": "^2.32.0",
    "@gravity-ui/react-data-table": "^2.1.1",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@remixicon/react": "^4.6.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.4",
    "axios": "^1.7.9",
    "bem-cn-lite": "^4.1.0",
    "clsx": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "firebase": "^11.2.0",
    "framer-motion": "^12.3.1",
    "next": "^15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^4.0.4",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.3.0",
    "@gravity-ui/chartkit": "^5.20.0",
    "@gravity-ui/components": "^4.0.0",
    "@gravity-ui/date-components": "^2.12.0",
    "@gravity-ui/eslint-config": "^3.3.0",
    "@gravity-ui/illustrations": "^2.0.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@gravity-ui/uikit": "^7.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "stylelint": "^16.14.1"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.1.4"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "lint-staged": {
    "*.{scss}": [
      "bunx stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "bunx eslint --fix --quiet",
      "bunx prettier --write"
    ],
    "*.md": [
      "bunx prettier --write"
    ]
  }
}